      }
    },
    "schemas": {
      "ResultData": {
        "type": "object",
        "properties": {
          "status": {
            "type": "number",
            "default": 0,
            "description": "状态码"
          },
          "message": {
            "type": "string",
            "default": "OK",
            "description": "状态描述"
          },
          "data": {
            "type": "object",
            "description": "返回结果"
          }
        },
        "required": [
          "status",
          "message",
          "data"
        ]
      },
      "UserInfoResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "用户ID"
          },
          "code": {
            "type": "string",
            "description": "用户编号"
          },
          "name": {
            "type": "string",
            "description": "用户名称"
          }
        },
        "required": [
          "id",
          "code",
          "name"
        ]
      },
      "User": {
        "type": "object",
        "properties": {}
      },
      "Pagination": {
        "type": "object",
        "properties": {
          "page": {
            "type": "number",
            "description": "页码"
          },
          "limit": {
            "type": "number",
            "description": "每页数量"
          },
          "sortBy": {
            "type": "string",
            "description": "排序字段"
          },
          "order": {
            "type": "string",
            "description": "排序方式"
          }
        }
      },
      "UserPageQueryDto": {
        "type": "object",
        "properties": {
          "pagination": {
            "description": "分页参数",
            "allOf": [
              {
                "$ref": "#/components/schemas/Pagination"
              }
            ]
          },
          "code": {
            "type": "string",
            "description": "用户编码"
          },
          "name": {
            "type": "string",
            "description": "用户姓名"
          },
          "email": {
            "type": "string",
            "description": "用户邮箱"
          }
        }
      },
      "SignInResponseDto": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "description": "登录令牌"
          }
        },
        "required": [
          "token"
        ]
      },
      "SignInRequestDto": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "用户编号"
          },
          "password": {
            "type": "string",
            "description": "登录密码"
          },
          "bindKey": {
            "type": "string",
            "description": "验证码绑定键"
          },
          "captcha": {
            "type": "string",
            "description": "验证码"
          }
        },
        "required": [
          "code",
          "password",
          "bindKey",
          "captcha"
        ]
      },
      "ImageCaptchaResponseDto": {
        "type": "object",
        "properties": {
          "bindKey": {
            "type": "string",
            "description": "验证码绑定键"
          },
          "image": {
            "type": "string",
            "description": "验证码SVG图片"
          }
        },
        "required": [
          "bindKey",
          "image"
        ]
      }
    }
  },
